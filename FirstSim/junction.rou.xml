<?xml version="1.0" encoding="UTF-8"?>
<routes>
    <!-- One simple car type -->
    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5" minGap="2.5" maxSpeed="13.9" guiShape="passenger"/>

    <!-- 4 routes that match YOUR net edges -->
    <route id="north_south" edges="north_in south_out"/>
    <route id="south_north" edges="south_in north_out"/>
    <route id="east_west"   edges="east_in west_out"/>
    <route id="west_east"   edges="west_in east_out"/>

    <!-- Lower volumes, ~300 vph per approach (â‰ˆ1 car / 12s) -->
    <flow id="f_ns" type="car" route="north_south" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="f_sn" type="car" route="south_north" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="f_ew" type="car" route="east_west"   begin="0" end="3600" vehsPerHour="300"/>
    <flow id="f_we" type="car" route="west_east"   begin="0" end="3600" vehsPerHour="300"/>
</routes>
